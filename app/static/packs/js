/*
  Minimal test SDK for local/integration testing.
  Exposes window.chatwootSDK.run() and fires chatwoot:ready.
  NOT for production use (only for debugging/integration).
*/
(function(){
  if (window.chatwootSDK) return; // evitar doble carga
  window.chatwootSDK = {
    run: function(opts){
      try {
        console.log('[minimal-sdk] run called with', opts);
        // guarda params por si alguien los consulta
        window.chatwootSDK._lastRun = opts || {};
        // simula inicializaci√≥n y disparo de evento ready
        setTimeout(function(){
          try {
            var ev = new Event('chatwoot:ready');
            window.dispatchEvent(ev);
            console.log('[minimal-sdk] dispatched chatwoot:ready');
          } catch(e){ console.warn('[minimal-sdk] ready dispatch failed', e); }
        }, 80);
      } catch(e){
        console.error('[minimal-sdk] run error', e);
      }
    },
    getUrl: function(){ return (window.chatwootSDK._lastRun && window.chatwootSDK._lastRun.baseUrl) || window.location.origin; },
    createFrame: function(){ console.warn('[minimal-sdk] createFrame called (noop)'); }
  };
})();
JS
